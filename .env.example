# Environment variables for testing
# Copy this file to .env and update the values as needed

# SQLite (default for tests)
DATABASE_URL="file:./tests/prisma/test.db"

# MySQL (for integration testing)
# Start with: docker-compose up -d mysql
DATABASE_URL_MYSQL="mysql://prisma_test:prisma_test_password@localhost:3311/prisma_test"

# PostgreSQL (for integration testing)
# Start with: docker-compose up -d postgresql
DATABASE_URL_POSTGRESQL="postgresql://prisma_test:prisma_test_password@localhost:5433/prisma_test"
